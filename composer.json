{
  "name": "robiningelbrecht/playstation-easy-platinums",
  "description": "This repository is a collection of easy and quick obtainable PlayStation platinum trophies",
  "keywords": [
    "README",
    "PlayStation"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Robin Ingelbrecht",
      "email": "ingelbrecht_robin@hotmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "adhocore/cli": "^0.9.1",
    "guzzlehttp/guzzle": "^7.4",
    "moneyphp/money": "^4.0",
    "twig/twig": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "spatie/phpunit-snapshot-assertions": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
